angeFile.bind(this)}\r\n        />\r\n        {this.state.isUploaded && (\r\n          <div className=\"HiddenText\">\r\n            {this.state.isPasswordProtected ? (\r\n              <div className=\"password-protected-content\">\r\n                <div className=\"password-container\">\r\n                  <div className=\"password-header\">\r\n                    <i className=\"fa fa-lock\"></i>\r\n                    <h3>This image is password protected</h3>\r\n                  </div>\r\n                  <input\r\n                    type=\"password\"\r\n                    className=\"password-input\"\r\n                    placeholder=\"Enter password\"\r\n                    value={this.state.password}\r\n                    onChange={this.handlePasswordChange.bind(this)}\r\n                    onKeyPress={this.handleKeyPress.bind(this)}\r\n                    disabled={this.state.isPasswordCorrect}\r\n                    autoFocus\r\n                  />\r\n                  {!this.state.isPasswordCorrect && (\r\n                    <Button\r\n                      text=\"Verify Password\"\r\n                      onClick={this.verifyPassword.bind(this)}\r\n                      disabled={!this.state.password}\r\n                    />\r\n                  )}\r\n                  {this.state.passwordError && (\r\n                    <div className=\"error-message\">{this.state.passwordError}</div>\r\n                  )}\r\n                </div>\r\n                {this.state.isPasswordCorrect && (\r\n                  <div className=\"decoded-content\">\r\n                    <h3>Decoded message:</h3>\r\n                    <div\r\n                      className=\"HiddenTextContainer\"\r\n                      dangerouslySetInnerHTML={this.replaceURLWithHTMLLinks(\r\n                        this.state.hiddenText\r\n                      )}\r\n                    />\r\n                  </div>\r\n                )}\r\n              </div>\r\n            ) : (\r\n              <div className=\"decoded-content\">\r\n                <h3>Decoded message:</h3>\r\n                <div\r\n                  className=\"HiddenTextContainer\"\r\n                  dangerouslySetInnerHTML={this.replaceURLWithHTMLLinks(\r\n                    this.state.hiddenText\r\n                  )}\r\n                />\r\n              </div>\r\n            )}\r\n          </div>\r\n        )}\r\n        <Button\r\n          text=\"Upload\"\r\n          size=\"big\"\r\n          onClick={() => {\r\n            this.upload.click();\r\n          }}\r\n          disabled={this.state.isLoading}\r\n        />\r\n        {this.state.isLoading && (\r\n          <div className=\"loading-container\">\r\n            <div className=\"loading-spinner\"></div>\r\n            <div className=\"loading-text\">\r\n              {this.state.processingStatus}\r\n            </div>\r\n          </div>\r\n        )}\r\n        <subtitle>Large images will be automatically compressed for better performance.</subtitle>\r\n        {this.state.haveError && (\r\n          <div className=\"ErrorPanel\">\r\n            Oh! Something happened. It might be because your image is corrupted\r\n            or we messed up something. Either way, you can't use this image.\r\n          </div>\r\n        )}\r\n      </div>\r\n    );\r\n  }\r\n}\r\n","C:\\Users\\vedan\\OneDrive\\Desktop\\finalproject\\stegano\\src\\pages\\About.js",[],[],"C:\\Users\\vedan\\OneDrive\\Desktop\\finalproject\\stegano\\src\\helpers\\Steganography.js",[],[],"C:\\Users\\vedan\\OneDrive\\Desktop\\finalproject\\stegano\\src\\components\\Button.js",[],[],"C:\\Users\\vedan\\OneDrive\\Desktop\\finalproject\\stegano\\src\\components\\TextArea.js",[],[],"C:\\Users\\vedan\\OneDrive\\Desktop\\finalproject\\stegano\\src\\pages\\Login.js",[],[],{"ruleId":"107","severity":1,"message":"108","line":46,"column":50,"nodeType":"109","messageId":"110","endLine":46,"endColumn":51,"suggestions":"111"},{"ruleId":"107",